{
    "name": "zxin/express",
    "type": "library",
    "authors": [
        {
            "name": "zxin"
        }
    ],
    "keywords": ["express", "17track"],
    "autoload": {
        "psr-4": {
            "Zxin\\Express\\": "src/"
        }
    },
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "composer/ca-bundle": "^1.3",
        "symfony/polyfill-php80": "^1.25.0",
        "symfony/polyfill-php81": "^1.25.0",
        "guzzlehttp/guzzle": "^7.4.2",
        "nesbot/carbon": "^2.57.0"
    },
    "require-dev": {
        "symfony/var-dumper": "^5.4|^6"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": false,
        "platform": {
            "php": "7.4"
        }
    },
    "minimum-stability": "stable"
}
